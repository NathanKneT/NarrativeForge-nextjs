{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-console": "off",
    "react/no-unescaped-entities": "off",
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "no-undef": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "**/*.test.*"
  ]
}